<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".linearLayoutss"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="37dp"
            android:layout_height="34dp"
            android:contentDescription="TODO"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.041"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/ellipse_1"
            tools:ignore="ContentDescription,HardcodedText,MissingConstraints"
            tools:layout_editor_absoluteY="16dp" />

        <EditText
            android:id="@+id/editTextText3"
            android:layout_width="265dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="text"
            tools:ignore="LabelFor,SpeakableTextPresentCheck"
            tools:layout_editor_absoluteX="82dp"
            tools:layout_editor_absoluteY="-3dp"
            android:importantForAutofill="no" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="44dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/account_avatar_face_man_people_profile_user_icon_123197"
            tools:ignore="ContentDescription,MissingConstraints"
            tools:layout_editor_absoluteX="342dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/help_question_4509"
            tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints"
            tools:layout_editor_absoluteX="369dp"
            tools:layout_editor_absoluteY="14dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Иван,"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="48dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="приветствуем в нашем приложении"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="67dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="352dp"
        android:layout_height="125dp"
        app:srcCompat="@drawable/group_1"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="93dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="140dp"
        android:layout_height="44dp"
        android:backgroundTint="#751FC1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.331"
        tools:ignore="DuplicateSpeakableTextCheck,HardcodedText,MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Предложения"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="283dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="363dp"
        android:layout_height="109dp"
        app:srcCompat="@drawable/group_2"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="311dp"
        tools:ignore="ContentDescription,MissingConstraints" />

    <Button
        android:id="@+id/button3"
        android:layout_width="140dp"
        android:layout_height="44dp"
        android:backgroundTint="#751FC1"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.628"
        tools:ignore="HardcodedText,MissingConstraints,TouchTargetSizeCheck,TextSizeCheck" />

    <Button
        android:id="@+id/button4"
        android:layout_width="140dp"
        android:layout_height="44dp"
        android:backgroundTint="#751FC1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.712"
        tools:ignore="HardcodedText,MissingConstraints,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="174dp"
        android:layout_height="164dp"
        app:srcCompat="@drawable/priroda"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="542dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Подробная информация"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="706dp"
        tools:ignore="HardcodedText,MissingConstraints" />

</androidx.appcompat.widget.LinearLayoutCompat>